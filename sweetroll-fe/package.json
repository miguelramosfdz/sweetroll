{
  "name": "sweetroll-fe",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "deps": "mkdir -p bower_components; ln -s ../../indieweb-components bower_components/indieweb-components; bower update",
    "assets": "rm -rf dist.new; broccoli build dist.new && mkdir -p dist && rsync -au --inplace --remove-source-files dist.new/ dist && rm -rf dist.new && echo '==== Built assets'",
    "lint": "eslint --ignore-path .gitignore . && echo ESLint OK; stylelint assets/*.css && echo Stylelint OK; true",
    "fmt": "stylefmt assets/*.css",
    "devserve": "prok"
  },
  "processes": {
    "app": "DEBUG=*,-koa-mount,-dynamic-asset-rev LIVE_RELOAD=1 IS_PROXIED=1 INDIEAUTH_ENDPOINT=/login/self WEBSUB_HUB=https://httpbin.org/post MICRO_PANEL_ROOT=/micro-panel node-dev --harmony index.js",
    "lr": "tiny-lr",
    "re-asset": "chokidar 'assets/**/*' 'Brocfile.js' --initial -p -c 'npm run assets; curl -s -X POST \"localhost:35729/changed?files=dist\"; true'",
    "re-tpl": "chokidar 'views/**/*' -p -c 'curl -s -X POST \"localhost:35729/changed?files=dist\"; true'"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "bower": "^1.8.0",
    "broccoli": "^1.1.1",
    "broccoli-brotli": "^1.0.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-concat": "^3.2.2",
    "broccoli-config-replace": "^1.1.2",
    "broccoli-funnel": "^1.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-postcss": "^3.3.0",
    "broccoli-pug-render": "^1.0.4",
    "broccoli-source-map": "^0.2.4",
    "broccoli-static-asset-rev": "^0.1.1",
    "broccoli-svgstore": "^0.4.2",
    "broccoli-zopfli": "github:myfreeweb/broccoli-zopfli",
    "chokidar-cli": "^1.2.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "make-livereload": "^0.1.2",
    "node-dev": "^3.1.3",
    "postcss-color-function": "^3.0.0",
    "postcss-css-variables": "^0.7.0",
    "postcss-flexbugs-fixes": "^2.1.1",
    "postcss-nesting": "^2.3.1",
    "postcss-responsive-type": "^0.5.1",
    "prok": "0.0.8",
    "stylefmt": "^5.3.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "debug": "^2.6.6",
    "dotenv": "^4.0.0",
    "dynamic-asset-rev": "^0.1.0",
    "emoji-king": "^0.1.0",
    "gravatar-url": "^2.0.0",
    "http-link-header": "^0.8.0",
    "jsonwebtoken": "^7.4.0",
    "koa": "^2.2.0",
    "koa-better-router": "^2.1.1",
    "koa-better-serve": "^1.0.0",
    "koa-cash": "next",
    "koa-compose": "^4.0.0",
    "koa-mount": "^3.0.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "node-fetch": "^1.6.3",
    "pg": "^6.1.5",
    "pg-async": "^2.4.3",
    "pify": "^2.3.0",
    "promised-retry": "^0.2.3",
    "pug": "^2.0.0-beta12",
    "pug-plugin-try-catch": "^1.2.0",
    "sse-broadcast": "^0.4.1",
    "urijs": "^1.18.10"
  }
}
